{
  "name": "@apigear-io-templates/cpp14-blueprint",
  "version": "2.1.1",
  "description": "A pure C++ 14 code generator",
  "keywords": [
    "objectapi",
    "library",
    "client"
  ],
  "devDependencies": {
    "@apigear-io/apigear-cli": "^0.3.3",
    "standard-version": "^9.5.0"
  },
  "scripts": {
    "gen:goldenmaster": "rm -rf goldenmaster && node node_modules/@apigear-io/apigear-cli/bin/run gen run apigear/goldenmaster.solution.yaml && mv test/ goldenmaster",
    "test:goldenmaster": "rm -rf test && node node_modules/@apigear-io/apigear-cli/bin/run gen run apigear/goldenmaster.solution.yaml && diff -r goldenmaster/ test/",
    "report:license": "license-report --exclude=@apigear-io/apigear-cli --department.value=apigear --relatedTo.value=$npm_package_name --output=csv --csvHeaders > licenses.csv",
    "version": "standard-version -i CHANGELOG.md --same-file"
  },
  "author": {
    "name": "ApiGear UG",
    "url": "https://github.com/apigear-io/template-cpp14/issues"
  },
  "homepage": "https://github.com/apigear-io/template-cpp14#readme",
  "license": "SEE LICENSE IN LICENSE.TXT",
  "files": [
    "/templates",
    "/rules.yaml",
    "/README.md",
    "/LICENSE.txt"
  ],
  "publishConfig": {
    "registry": "https://europe-west3-npm.pkg.dev/apigear/technology-templates/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apigear-io/template-cpp14.git"
  },
  "bugs": {
    "url": "https://github.com/apigear-io/template-cpp14/issues"
  }
}
