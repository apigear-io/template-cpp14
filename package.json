  "scripts": {
    "gen:goldenmaster": "rm -rf goldenmaster && node node_modules/@apigear-io/objectapi-cli/bin/apigear-gen --rules rules.yaml --force --sources apis/testbed.module.yaml --features all --output test && mv test/ goldenmaster",
    "test:goldenmaster": "rm -rf test && node node_modules/@apigear-io/objectapi-cli/bin/apigear-gen --rules rules.yaml --force --sources apis/testbed.module.yaml --features all --output test && diff -r goldenmaster/ test/"
  },
